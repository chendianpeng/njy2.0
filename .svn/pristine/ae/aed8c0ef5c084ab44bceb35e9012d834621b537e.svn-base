    <!--发货信息-->
    <div class="order-fh">
        <p>发货信息</p>
        <div class="order-fh-mian">
            <span class="fhfs">发货方式 <i class="red">*</i></span>
            <div class="wlfs">
                <div class="wlfs1">
                    <input type="radio" id="dsf" name="wl" ng-click="wlCheck(0)" checked="checked">
                    <label for="dsf">第三方物流</label>
                </div>
                <div class="wlfs1">
                    <input type="radio" id="sr" name="wl" ng-click="wlCheck(1)">
                    <label for="sr">私人司机</label>
                </div>
            </div>
            <div class="wlxx" ng-show="wl">
                <form action="">
                    <p class="p1">
                        <label for="wlgs">物流公司:</label>
                        <input type="text" id="wlgs">
                    </p>
                    <label for="yjsj">预计到达司机:</label>
                    <input type="text" id="yjsj">
                    <br>
                    <label for="wldh">物流单号:</label>
                    <input type="text" id="wldh">
                </form>
            </div>
            <div class="wlxx" ng-hide="wl">
                <form action="">
                    <p class="p1">
                        <label for="sjxm">司机姓名:</label>
                        <input type="text" id="sjxm">
                    </p>
                    <p class="p1">
                        <label for="cphm">车牌号码:</label>
                        <input type="text" id="cphm">
                    </p>
                    <p class="p1">
                        <label for="yjdate">预计到达时间:</label>
                        <input type="text" id="yjdate">
                    </p>
                    <p class="p1">
                        <label for="sjhm">手机号码:</label>
                        <input type="text" id="sjhm">
                    </p>
                    <p class="p1">
                        <label for="clwg">车辆外观:</label>
                        <input type="text" id="clwg">
                    </p>
                </form>
            </div>
            <div class="fhpz">
                <p>发货凭证</p>
                <form action="">
                    <div id="showImgGroup" class="showImgGroup"></div>
                </form>
                <div class="lr">例如:快递单、物流单、装车照片、货品照片等，最多五张</div>
            </div>
            <button class="fh-btn" ng-click="fhClick()">确认发货</button>
        </div>
    </div>

</div>
<!--尾部-->
<div njy-pagefoot></div>


<!--确认发货弹窗-->
<div class="qrA-body" ng-show="isShow">
    <div class="qrA">
        发货成功
