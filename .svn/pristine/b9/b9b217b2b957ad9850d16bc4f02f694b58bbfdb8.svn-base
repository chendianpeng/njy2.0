/*
卖家发货-退款中订单阶段*/

/*等待供应商退款中*/
.content-header-l strong>i{
    display: inline-block;
    font-size: 12px;
    color: #999;
}
/*进度条退款中*/
.jdt ul > li .tkz{
    width: 60px;
    height: 20px;
    background-color: #cbeeff;
    color: #0984c0;
    position: absolute;
    font-size:12px;
    top:-33px;
    left:20px;
    line-height: 20px;
    text-align: center;
}
/*第三方物流*/
.order-fh .order-fh-mian .wlfs .wlfs1>#dsf {
    margin-right: 4px;
    position: relative;
    width: 15px;
    height: 15px;
    border-radius: 50%;
    border:1px solid #666666;
    background-color: #cccccc;

}
/*私人司机*/
.order-fh .order-fh-mian .wlfs .wlfs1>#sr {
    margin-right: 4px;

    position: relative;
    width: 15px;
    height: 15px;
    border-radius: 50%;
    border:1px solid #666666;
    background-color: #f2f2f2;
}
.order-fh .order-fh-mian .wlfs .wlfs1>button{
    top: 0px;
    left: -2px;
    cursor: auto;
}
.order-fh .order-fh-mian .wlxx input {
    border:1px solid #DCDCDC;
    background-color: #f2f2f2;
}

/**/
.order-fh .order-fh-mian .fhpz .fhpz-img {
    width: 86px;
    height: 86px;
    background-color: #f2f2f2;
    border:1px solid #DCDCDC;
    color: #999;
    font-size: 12px;
    line-height: 86px;
    text-align: center;
}

/*商品总金额*/
.order-mian-spxx > .spxx-list .zje {
    text-align: right;
    padding: 20px 0px 14px 0px;
    font-size:12px;
}
.order-mian-spxx > .spxx-list .zje>strong{
    color: #333;
}
