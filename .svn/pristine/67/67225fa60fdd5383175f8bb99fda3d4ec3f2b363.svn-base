/*买家收货*/
body {
    background-color: #ffffff;
    position: relative;
}
.content-header-l {
    width: 301px;
    height: 128px;
    border-right: 1px solid #e9e9e9;
    float: left;
    padding: 20px 170px 0px 40px;
    position: relative;
}

.content-header-l .qrsh {
    width: 100px;
    height: 30px;
    line-height: 30px;
    border: none;
    background-color: #2c99cd;
    color: #FFFFFF;
    font-weight: bolder;
    border-radius: 2px;
    font-size: 14px;
    position: absolute;
    right:40px;
    bottom:30px;
}

/*配送信息*/
.order-main .order-sh-ps {
    padding-top: 20px;
    border-bottom: 1px solid #E2E2E2;
    font-size: 12px;
    color: #333;
    padding-bottom: 20px;
}

.order-main .order-sh-ps .gy {
    font-size:14px;
    margin-bottom: 20px;
}
.order-main .order-sh-ps>.gy-l{
    display: inline-block;
    padding-right: 55px;
    border-right:1px solid #ccc;
}
.order-main .order-sh-ps>.gy-l.brn{
    border-right: none;
}
.p55{
    padding-left: 55px;
}
.order-main .order-sh-ps>.gy-l>p{
    padding-bottom: 13px;
}
.order-main .order-sh-ps>.gy-l>p.p0{
    padding-bottom: 0px;
}
.order-main .order-sh-ps>.gy-l>p>i{
    display: inline-block;
    width: 67px;
}
.order-main .order-sh-ps>.gy-l>p>i.w88{
    width: 88px;
}
/*发货凭证*/
.order-mian-pz{
    padding: 20px 0px 20px 0px;
    border-bottom: 1px solid #ccc;
}
.order-mian-pz .pz{
    font-size:14px;
    margin-bottom: 20px;
}
.order-mian-pz .pz-img{
    display: inline-block;
    width: 80px;
    height: 80px;
    margin-right: 10px;
}
.order-mian-pz .pz-img>img{
    width: 100%;
    height: 100%;
}

