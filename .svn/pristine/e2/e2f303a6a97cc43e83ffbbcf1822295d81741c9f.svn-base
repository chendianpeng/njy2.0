.hpmsDiv img {
    margin-left:50px;
}
/*遮罩层*/
.box{
    position:fixed;
    top:0;
    bottom:0;
    left:0;
    right:0;
    background:#000;
    opacity:0.7;
    filter:alpha(opacity=70);
    z-index:100;
    cursor:pointer;
}
/*弹出框*/
.winDiv {
    width:490px;
    height:250px;
    padding:30px;
    position:fixed;
    left:50%;
    top:50%;
    z-index:300;
    background-color:#fff;
    margin-left:-245px;
    margin-top:-125px;
}
.winDiv ul li p {
    display:inline-block;
}
.winDiv ul li {
    margin-bottom:15px;
    font-size:14px;
}
.winDiv ul li span {
    display: inline-block;
    margin-right:20px;
    color:#999;
}
.pjlDiv {
    float:left;
    padding:12px;
    border-right:1px solid #ddd;
}
.pjlDiv h3,.wjlDiv h3 {
    font-weight: normal;
    font-size:16px;
    margin-bottom:20px;
}
.wjlDiv {
    float:left;
    padding:12px;
    margin-bottom:20px;
}
.closeBtn {
    display:block;
    clear:both;
    width:175px;
    height:46px;
    line-height:46px;
    color:#fff;
    text-align:center;
    border-radius:3px;
    background-color:#2E99CD;
    cursor:pointer;
    margin:0 auto;
}
.closeBtn:hover {
    color:#fff!important;
}
.dpn {display:none;}
.dpb {display:block;}
