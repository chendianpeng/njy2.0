}
.linkAp:hover {
 color:#fff!important;
}
/*供应商部分*/
.main {
    width:1200px;
    height:auto;
    margin:0 auto;
    margin-top:30px;
}
.gys {
    width:298px;
    height:352px;
    border:1px solid #ddd;
    font-size:14px;
    float:left;
}
.gys span {
    font-size:14px;
}
.gys hr {
    overflow:hidden;
    width:86%;
    /*background-color:#E2E2E2;*/
    border:1px solid #E2E2E2;
    margin:0 auto;
}
.apr {
    border:1px solid #E2E2E2;
    background-color:#F1F1F1;
    height:42px;
    line-height:42px;
    padding-left:15px;
    font-size:16px;
    color:#363636;
}
.gysName {
    height:54px;
    padding:20px 10px 10px 10px;
}
.gysName img {
    float:left;
    margin-left:25px;
    width:50px;
    height:50px;
}
.gysName span {
    /*float:left;
    line-height:50px;*/
    float:left;
    display:-webkit-box;
    height:40px;
    /*width:220px;*/
    width:188px;
    text-overflow:ellipsis;
    /*white-space: nowrap;*/
    overflow:hidden;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
    line-height:23px;
    margin-left:5px;
    font-size:15px;
}
.supSpan {
    display:inline-block;
    width:155px;
}
.ovfHid {
    text-overflow:ellipsis;
    white-space: nowrap;
    overflow:hidden;
    display: inline;
}
.gysInfo {
    /*padding:10px;*/
    margin-top:24px;
}
.gysInfo p {
    margin-bottom:18px;
    margin-left:35px;
}
.fwzy {
    display: block;
    /*width:170px;
    height:30px;*/
    width:193px;
    height:34px;
    padding:3px 15px;
    text-align:center;
    background-color:#D7E4EC;
    line-height:34px;
    border:1px solid #2290C5;
    border-radius:5px;
    margin:0 auto;
    color:#168AC2;
    cursor:pointer;
}
.fwzy:visited {
    color:#168AC2;
}
.fwzy span {
    display: inline-block;
    vertical-align: middle;
}
.fwzy i {
    display: inline-block;
    width:7px;
    height:12px;
    background:url("../img/left_ar.png") no-repeat;
    vertical-align: middle;
    margin-left:10px;
}
