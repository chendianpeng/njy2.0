body{
    font-size: 12px;
    font-family: 'SimSun';
}
a{
    color:#0984c0;
}
a:visited{
    color:#0984c0;
}
table{
    width: 1200px;
    margin: 0 auto;
    margin-bottom: 20px;
}
.bg-f1{
    background-color: #f1f1f1;
}
.bg-f6{
    background-color: #fff6f0;
}
.bg-f5{
    background-color: #f5f5f5;
    color:#999;
}
.vat{
    vertical-align: top;
}
tr{
    height: 44px;
    border: 1px solid #e2e2e2;
}
.tr-h120{
    height: 120px;
}
.td-lf{
    padding-left: 20px;
}
.td-rt{
    padding-right: 20px;
}
.td-w120{
    width: 120px;
}
.td-w400{
    width: 400px;
}
.s-w290{
    display:inline-block;
    width: 290px;
}
.td-xj{
    font-weight: bold;
    color:#ff0000;
}
button{
    height: 100%;
    width:20px;
}
.s-btn{
    display: inline-block;
    width:20px;
    height: 100%;
    background-color:#f1f1f1 ;
    text-align: center;
    border-right:1px solid #e2e2e2;
    cursor: pointer;
}
.h80{
    height: 80px;
}
.s-val{
    width:48px;
    height: 22px;
    text-align: center;
    border: none;
    vertical-align: top;
}
.numb{
    width:90px;
    height: 24px;
    border: 1px solid #E2E2E2;
    border-right: 0;
    line-height: 24px;
}
td>.h80>p{
    line-height: 20px;
    height:25px;
    color:#999;
}
input[type='checkbox'] {
    display: none;
}
label {
    display: inline-block;
    width: 12px;
    height: 12px;
    vertical-align: top;
    border: 1px solid #E2E2E2;
    background-color: #fff;
}
.bg-e{
    background-color: #eee;
}
.bg-yes{
    background: url('./../img/images/little.png') no-repeat 0 0;
}
td>.h80>.ft-3{
    color: #333;
}
#total{
    width:1200px;
    margin: 0 auto;
    height:70px;
    border:1px solid #e2e2e2;
    background-color: #f1f1f1;
}
.total-rt{
    background-color: #ff7800;
    width: 156px;
    height: 70px;
    float: right;
    text-align: center;
    line-height: 72px;
    font-size: 20px;
    color:#fff;
    font-weight:bold;
    cursor: pointer;
}
.fl-r{
    float: right;
    font-size: 16px;
    color:#999;
    height: 100%;
    text-align: center;
    line-height: 72px;
    margin-right: 30px;
}
.bg-ec1818{
    color: #ec1818;
    font-size: 30px;
}
.total-lf{
    height: 50px;
    margin: 0 20px 20px 20px;
    float: left;
    position: relative;
}
.total-lf label{
    position: absolute;
    left:0;
    bottom: 0;
}
.total-lf span{
    display: inline-block;
    width: 26px;
    position: absolute;
    left:20px;
    bottom: 1px;
}
.total-lf a{
    position: absolute;
    left: 70px;
    bottom: 1px;
    width: 90px;
}
/*购物车*/
#no-cart{
    width: 1200px;
    height:560px;
    background-color: #f9f9f9;
    margin: 0 auto;
    position: relative;
}
#no-cart img{
    position: absolute;
    left:23%;
    top:20%;
}
#no-cart p{
    width: 400px;
    height: 100px;
    position: absolute;
    left:46%;
    top:30%;
    font-size: 20px;
    line-height: 40px;
}