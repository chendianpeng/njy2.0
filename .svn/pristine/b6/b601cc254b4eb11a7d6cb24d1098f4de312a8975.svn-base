.supplier_List .left{
    width: 300px;
    min-height: 294px;
    border: 1px solid #e2e2e2;
    float: left;
}
.supplier_List .right{
    width: 880px;
    float: right;
}

.supplier_List .left h1{
    height: 44px;
    line-height: 44px;
    background-color: #f1f1f1;
    border-bottom: 1px solid #e2e2e2;
    padding-left: 20px;
    font-weight: 500;
    color: #333;
}
.supplier_List .left .mes{
    padding:20px 17px 0 14px;
}
.supplier_List .left .mes .supplier_mes{
    padding: 0 20px 13px 20px;
    border-bottom:1px solid #e2e2e2;
    overflow: hidden;
}
.supplier_List .left .mes .supplier_mes img{
    float: left;
}
.supplier_List .left .mes .supplier_mes .name{
    font-size: 16px;
    color: #333333;
    float: left;
    width: 160px;
    margin-left: 18px;
    line-height: 1.5;
}
.supplier_List .left .mes .info{
    padding: 0 20px;
    font-size: 14px;
    margin-top: 10px;
}
.supplier_List .left .mes .info .item{
    line-height: 2.5;
    clear: both;
}
.supplier_List .left .mes .info .key{
    color: #999;
    width: 60px;
    display: inline-block;
    float: left;
}
.supplier_List .left .mes .info .val{
    color: #333;
}
.supplier_List .left .mes .info .val2{
    display: inline-block;
    width: 160px;
}
.supplier_List .right .search{
    height: 44px;
    line-height: 44px;
    background-color: #f1f1f1;
    border: 1px solid #e2e2e2;
    padding-left: 20px;
    font-size: 12px;
}
.supplier_List .right .search p{
    color: #999;
    display: inline-block;
}
.supplier_List .right .search .result{
    color: #666;
}
.supplier_List .right .search .data{
    display: inline-block;
    float: right;
    margin-right: 23px;
}
.supplier_List .right .search .data .inputBox{
    display: inline-block;
    position: relative;
    width: 110px;
    height: 25px;
    line-height: 25px;

}
.supplier_List .right .search .data .inputBox input{
    width: 100%;
    border: 1px solid #e2e2e2;
    padding: 0;
    height: 24px;
}
.supplier_List .right .search .searchBtn{
    width: 58px;
    height:24px;
    line-height: 23px;
    border: 1px solid #cccccc;
    background-color: #fff;
    font-size: 12px;
    padding:0;
    margin-left: 3px;

}
.supplier_List .right .selection{
    padding-left: 20px;
    font-size: 0;
    border: 1px solid #e2e2e2;
    border-top: none;
    height: 44px;
    line-height: 44px;
}

.supplier_List .right .selection span{
    font-size: 12px;
    color:#666;
    margin-right: 18px;
    cursor: pointer;
}
.supplier_List .right .selection .select,
.supplier_List .right .selection .select2{
    color:#0984c0;
}
.supplier_List .right .selection .select .icon{
    background-position:-34px -7px;
}
.supplier_List .right .selection .select2 .icon{
    background-position:-8px -23px;
}
.supplier_List .right .selection .icon{
    background: url('../../res/img/list_icon.png') no-repeat;
    display: inline-block;
    width: 15px;
    height: 15px;
    background-position:-57px -7px;
}
.supplier_List .right .selection .icon2{
    background: url('../../res/img/list_icon.png') no-repeat;
    display: inline-block;
    width: 15px;
    height: 15px;
    background-position:-34px -24px;
}
.supplier_List .commodity .item{
    width: 203px;
    height: 338px;
    margin:0 20px 20px 0;
    border:1px solid #fff;
    float:left;

}
.supplier_List .commodity .item5{
    margin-right:0 ;
}
.supplier_List .commodity .item:hover{
    border:1px solid #dddddd;
}
