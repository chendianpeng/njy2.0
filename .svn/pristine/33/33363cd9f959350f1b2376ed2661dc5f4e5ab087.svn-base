/*foot*/

@charset "utf-8";
#loginFooter {
    background: #333;
    width: 100%;
    overflow: hidden;
    color: #fff;
}

#footerText {
    width: 1102px;
    margin: 0 auto 20px auto;
    padding: 70px 2px 25px 2px;
    border-bottom: 1px #555555 solid;
	text-align:center;
}

#footerText a {
    padding-right: 49px;
    color: #fff;
    text-decoration: none;
    display: inline-block;
    margin-top: 10px;
    font-size: 20px;
}

#footerText a:last-child {
    padding-right: 0;
}

#loginFooter p {
    text-align: center;
    font-size: 14px;
    padding: 30px 0 80px 0;
}

#loginFooter p a {
    color: #fff;
    display: inline-block;
    padding: 0 10px;
}

#loginFooter p span {
    color: #fff;
    display: inline-block;
    padding-left: 30px;
}
