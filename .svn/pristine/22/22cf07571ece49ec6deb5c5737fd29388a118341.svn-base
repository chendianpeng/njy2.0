@charset "utf-8";
.r_main .r_main_content{
	margin-top:68px;
}
.r_main .supplement,
.r_main .chenking,
.r_main .updateInfo{
	margin-top:100px;
	text-align:center;
}
.r_main .supplement a,
.r_main .updateInfo a{
	color:#2c99cd;
}
.r_main .r_main_content .title_box{
	float:left;
	width:150px;
}
.r_main .r_main_content .title_box span{
	width:130px;
	display:block;
	text-align:right;
	font-size:14px;
	color:#666;
	height:36px;
	line-height:36px;
	margin-bottom:20px;
}
.r_main .r_main_content .form_box{
	float:left;
	width:550px;
}
.r_main .r_main_content .form_box span{
	display:block;
	width:100%;
	height:36px;
	margin-bottom:20px;
}
.r_main .r_main_content .form_box input{
	/*float:left;*/
	width:500px;
	height:30px;
	line-height:30px;
	padding-left:5px;
}
.r_main .r_main_content .form_box select{
	float:left;
	width:162px;
	height:36px;
	line-height:36px;
	margin-bottom:20px;
	margin-right:10px;
	padding-left:2px;
}
.r_main .r_main_content .form_box label,
.r_main .r_main_content .form_box p{
	display:block;
	margin-bottom:20px;
}
.r_main .r_main_content .form_box .btn{
	font-family:"宋体";
	margin-top:20px;
	width:120px;
	height:40px;
	line-height:40px;
	display:inline-block;
	text-align:center;
	color:#fff;
	font-size:14px;
	padding:0;
	background-color:#2c99cd;
	border-radius:2px;
	cursor:pointer;
	vertical-align:middle;
	border:none;
}
.r_main .r_main_content .form_box .submitInfo[disabled]{
	cursor:not-allowed;
}
.r_main .r_main_content .form_box .showImg{
	width:86px;
	height:86px;
	margin-bottom:10px;
}
.r_main .r_main_content .form_box .userImg{
	display:block;
	width:86px;
	height:86px;
	margin-bottom:10px;
}
.r_main .r_main_content .edit input{
	ime-mode: disabled;
	disabled : disabled;
}
.r_main_content .errorMsg{
	color:red;
	display:inline-block;
}
.gryColor,
.r_main .r_main_content .form_box select.ng-pristine{
    color:#757575;
}
.r_main .r_main_content .form_box select.ng-dirty + select,
.r_main .r_main_content .form_box select.ng-dirty + select.ng-pristine + select
{
	color:#000;
}

.r_main .r_main_content .form_box option {
    color:#000;
}
.dpn {
    display: none;
}